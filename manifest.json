{
  "name": "Tab Resize",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "Automate resizing tab windows to predefined and user defined layouts.",
  "browser_action": {
    "default_icon": "icons/resize.png",
	"default_popup": "index.html",
	"default_title": "Tab Resize"
  },
  "icons": { "16": "icons/icon16.png",
           "48": "icons/icon48.png",
          "128": "icons/icon128.png" },
  "permissions": [
	"tabs"
  ]
 }